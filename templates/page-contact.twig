{% extends "base.twig" %}{% block header %}<nav class="fadeInDown animated" id="standardnav"><div data-equalizer="standardnav" class="row"><div data-equalizer-watch="standardnav" class="small-3 medium-4 columns"><a href="{{site.home}}"><img src="{{site.theme.link}}/assets/img/jm-logo.svg" /></a><span>DESIGNS</span></div><div data-equalizer-watch="standardnav" class="small-9 medium-8 columns"><ul class="menu">{% for item in topnav.get_items %}<li class="{{item.classes | join(' ')}}"><a href="{{item.link}}">{{item.title}}</a></li>{% endfor %}</ul></div></div></nav>{% endblock %}{% block content %}{% if post.contact_title %}<section class="fadeInUp animated" id="contact-text"><div class="row medium-8 centered columns">{% if post.contact_title %}<h2>{{post.contact_title}}</h2>{% endif %}{% if post.contact_text %}<p>{{post.contact_text}}</p>{% endif %}</div></section>{% endif %}{% if post.slug == 'contact' %}<section class="fadeInUp animated" id="contact-form"><div class="row"><div class="small-12 columnns"><h3>Send me an email</h3><div class="h2line">    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 772 5"><defs><style>.cls-1{fill:#ead975;opacity:0.53;}</style></defs><title>line-h2</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_3" data-name="Layer 3"><rect class="cls-1" width="772" height="3" transform="translate(772 5) rotate(180)"/></g></g></svg></div>{% filter shortcodes %}  [contact-form-7 id="62" title="Contact Me"]{% endfilter %}</div></div></section>{% endif %}{% endblock %}