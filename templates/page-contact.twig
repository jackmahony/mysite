{% extends "base.twig" %}{% block content %}{% if post.contact_title %}<section id="contact-text"><div data-equalizer="contact" class="row"><div data-equalizer-watch="contact" class="medium-4 columns"><a href="https://au.linkedin.com/in/jack-mahony-2aa9ba66" target="_blank"><div class="contact-1"><img src="{{site.theme.link}}/assets/img/linkedin-blue.svg" /><span>Connect with me on LinkedIn<br /></span></div></a><a href="https://dribbble.com/" target="_blank"><div class="contact-2"><img src="{{site.theme.link}}/assets/img/dribble-logo-black.svg" /><span>Follow me on dribble<br /></span></div></a></div><div data-equalizer-watch="contact" class="medium-7 medium-1-offset columns"><div class="medium-middle">{% if post.contact_title %}<h3>{{post.contact_title}}</h3>{% endif %}{% if post.contact_text %}<p>{{post.contact_text}}</p>{% endif %}</div></div></div></section>{% endif %}{% if post.slug == 'contact' %}<section id="contact-form"><div class="row"><div class="small-12 columnns"><h3>Send me an email</h3>{% filter shortcodes %}  [contact-form-7 id="62" title="Contact Me"]{% endfilter %}</div></div></section>{% endif %}{% endblock %}