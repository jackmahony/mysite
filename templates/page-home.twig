{% extends "base.twig" %}{% block content %}<section style="background: url('{{ TimberImage(post.header_banner) }}')  no-repeat;" id="portrait"><div class="row"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 210.97 264.49"><defs><style>.cls-1{fill:#fff;}</style></defs><title>Asset 1</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_1-2" data-name="Layer 1"><polygon class="cls-1" points="21.12 230.81 0 230.81 0 0 96.8 117.49 179.77 22.55 195.67 36.45 96.33 150.13 21.12 58.84 21.12 230.81"/><polygon class="cls-1" points="118.54 264.49 103.25 249.92 189.85 159.11 189.85 104.4 120.81 182.78 104.97 168.82 210.97 48.48 210.97 167.57 118.54 264.49"/></g></g></svg></div><div class="row">{% if post.tagline %}<h2>{{post.tagline}}</h2>{% endif %}</div></section><section id="services"><div class="row medium-up-2">    {% for item in post.get_field('my_services') %}<div class="column"><img src="{{TimberImage(item.icon)}}" /><h3>{{item.service_title}}</h3><p>{{item.about_services}}</p></div>    {% endfor %}</div></section><section id="latestwork"><div class="row"><div class="medium-12 columns">{% if post.projects_title %}<h2>{{post.projects_title}}</h2>{% endif %}</div></div>{% if post.show_projects %}<div data-equalizer="projects" class="row medium-up-3">{% for project in projects %}<div data-equalizer-watch="projects" class="column"><div class="medium-middle"><a href="{{project.link}}"><div style="background: url('{{ TimberImage(project.header_banner)}}') center,center;" class="singleproject"><div class="figcaption">{% if project.project_title %}<h4>{{project.project_title}}</h4><span class="info">View project &#10509;</span>{% endif %}</div></div></a></div></div>{% endfor %}</div>{% endif %}</section>{% endblock %}