{% extends "base.twig" %}{% block content %}<section style="background: url('{{ TimberImage(post.header_banner) }}')  no-repeat;" class="fadeInUp animated" id="portrait"><div class="row"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 210.97 264.49"><defs><style>.cls-1{fill:#fff;}</style></defs><title>Asset 1</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_1-2" data-name="Layer 1"><polygon class="cls-1" points="21.12 230.81 0 230.81 0 0 96.8 117.49 179.77 22.55 195.67 36.45 96.33 150.13 21.12 58.84 21.12 230.81"/><polygon class="cls-1" points="118.54 264.49 103.25 249.92 189.85 159.11 189.85 104.4 120.81 182.78 104.97 168.82 210.97 48.48 210.97 167.57 118.54 264.49"/></g></g></svg></div><div class="row">{% if post.tagline %}<h1>{{post.tagline}}</h1>{% endif %}</div></section><section id="services"><div class="row medium-up-2">    {% for item in post.get_field('my_services') %}<div class="column"><img src="{{TimberImage(item.icon)}}" /><h2>{{item.service_title}}</h2><p>{{item.about_services}}</p></div>    {% endfor %}</div></section>{% if post.show_projects %}<section class="bounceInUp animated" id="latestwork"><div class="row"><h2 class="latest">Some Of My Latest Work</h2></div><div data-equalizer="projects" class="row medium-up-3">{% for project in projects %}<div data-equalizer-watch="projects" class="column singleproject bounceInUp animated"></div>{% endfor %}</div></section>{% endif %}{% endblock %}