{% extends "base.twig" %}{% block content %}{% if post.about_title %}<section id="aboutme"><div data-equalizer="about" class="row"><div data-equalizer-watch="about" class="large-5 columns"><div class="medium-middle">{% if post.about_image %}<img src="{{TimberImage(post.about_image)}}" />{% endif %}</div></div><div data-equalizer-watch="about" class="large-7 columns"><div class="about-title">{% if post.about_title %}<h2>{{post.about_title}}</h2>{% endif %}{% for item in post.get_field('about_text') %}<p>{{item.text}}</p>{% endfor %}</div></div></div></section><section id="aboutskills"><div class="row"><div class="medium-12 columns"><h2>Technical Skills</h2></div></div><div class="row"><div class="medium-3 columns"><img src="{{site.theme.link}}/assets/img/50p.svg" /><h3>HTML/CSS</h3></div><div class="medium-3 columns"><img src="{{site.theme.link}}/assets/img/40p.svg" /><h3>WordPress</h3></div><div class="medium-3 columns"><img src="{{site.theme.link}}/assets/img/60p.svg" /><h3>Adobe PS/AI</h3></div><div class="medium-3 columns"><img src="{{site.theme.link}}/assets/img/50p.svg" /><h3>SolidWorks</h3></div></div></section>{% endif %}{% endblock %}