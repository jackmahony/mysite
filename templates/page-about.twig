{% extends "base.twig" %}{% block header %}<nav class="fadeInDown animated" id="standardnav"><div data-equalizer="standardnav" class="row"><div data-equalizer-watch="standardnav" class="small-3 medium-4 columns"><a href="{{site.home}}"><img src="{{site.theme.link}}/assets/img/jm-logo.svg" /></a><span>DESIGNS</span></div><div data-equalizer-watch="standardnav" class="small-9 medium-8 columns"><ul class="menu">{% for item in topnav.get_items %}<li class="{{item.classes | join(' ')}}"><a href="{{item.link}}">{{item.title}}</a></li>{% endfor %}</ul></div></div></nav>{% endblock %}{% block content %}{% if post.about_title %}<section class="fadeInUp animated" id="aboutme"><div class="row medium-8 centered columns"><div class="about-title">{% if post.about_title %}<h2>{{post.about_title}}</h2>{% endif %}{% for item in post.get_field('about_text') %}<p>{{item.text}}</p>{% endfor %}</div></div></section>{% endif %}<section class="fadeInUp animated" id="aboutskills"><div class="row"><div class="medium-12 columns"><h2>Technical Skills</h2></div><div class="h2line"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 772 5"><defs><style>.cls-1{fill:#ead975;opacity:0.53;}</style></defs><title>line-h2</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_3" data-name="Layer 3"><rect class="cls-1" width="772" height="3" transform="translate(772 5) rotate(180)"/></g></g></svg></div></div><div class="row medium-up-4">{% for item in post.get_field('dials') %}<div class="column"><div class="dials wow fadeInLeft"><img src="{{TimberImage(item.image).src}}" /><h3></h3></div></div>{% endfor %}<div class="h2line"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 772 5"><defs><style>.cls-1{fill:#ead975;opacity:0.53;}</style></defs><title>line-h2</title><g id="Layer_2" data-name="Layer 2"><g id="Layer_3" data-name="Layer 3"><rect class="cls-1" width="772" height="3" transform="translate(772 5) rotate(180)"/></g></g></svg></div></div></section>{% endblock %}