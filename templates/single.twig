{% extends "base.twig" %}{% block content %}<section id="projects"><div class="row"><div class="medium-12 columns">{% if post.project_title %}<h2>{{post.project_title}}</h2>{% endif %}</div></div><div class="row"><div class="medium-10 columns">{% for item in post.get_field('block_content') %}  {% if item.acf_fc_layout == 'text_block' %}    {% if item.heading_two %}<h2>{{item.heading_two}}</h2>    {% endif %}    {% if item.heading_three %}<h3>{{item.heading_three}}</h3>    {% endif %}    {% if item.body_text %}<p>{{item.body_text}}</p>    {% endif %}<hr />  {% elseif item.acf_fc_layout == 'gallery_block' %}    {% for image in item.gallery %}      {% if image %}<img src="{{TimberImage(image)}}" />      {% endif %}    {% endfor %}  {% endif %}{% endfor %}</div></div></section>{% endblock %}