{% extends "base.twig" %}{% block content %}<section style="background: url('{{ TimberImage(page.ink_mark) }}') top center no-repeat;" id="projects"><div class="row"><div class="medium-3 columns"><pre>{{dump(post.ink_mark)}}</pre>{% if post.hero_image %}<img src="{{TimberImage(post.hero_image).src|resize(600, 400)}}" />{% endif %}</div><div class="medium-8 columns">{% if post.project_title %}<h2>{{post.project_title}}</h2>{% endif %}{% if post.project_introduction %}<p>{{post.project_introduction}}</p>{% endif %}</div></div><div class="row"><div class="medium-12 columns">{% for section in post.get_field('project_content') %}  {% if section.acf_fc_layout == 'content' %}<section class="project-content">{{section.content_text}}</section>  {% elseif section.acf_fc_layout == 'gallery' %}<section class="project-gallery"><div class="row"><div class="large-12 columns"><div class="row small-up-1 medium-up-2">{% for image in section.content_gallery %}<div class="column"><img src="{{ TimberImage(image)|resize(600, 400) }}" /></div>{% endfor %}</div></div></div></section>  {% endif %}{% endfor %}</div></div></section>{% endblock %}