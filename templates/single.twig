{% extends "base.twig" %}{% block header %}<nav style="background: url('{{ TimberImage(post.header_banner) }}') no-repeat top;" id="project-nav"><div class="row"><div class="small-3 columns medium-6 columns"><div class="nav-logo"><img src="{{site.theme.link}}/assets/img/jm-logo.svg" /></div><span>Designs</span></div><div class="small-9 columns medium-6 columns"><div class="project-nav"><ul class="menu">{% for item in topnav.get_items %}<li class="{{item.classes | join(' ')}}"><a href="{{item.link}}">{{item.title}}</a></li>{% endfor %}</ul></div></div></div><div class="row small-middle">{% if post.project_logo %}<div class="proj-logo"><img src="{{TimberImage(post.project_logo)}}" /></div>{% endif %}</div></nav>{% endblock %}{% block content %}<section id="projects">{% for item in post.get_field('block_content') %}  {% if item.acf_fc_layout == 'text_block' %}<div class="row medium-6 medium-centered columns"><div class="textblock">{% if item.heading_two %}<h2>{{item.heading_two}}</h2>{% endif %}{% if item.body_text %}<p>{{item.body_text}}</p>{% endif %}</div></div>  {% elseif item.acf_fc_layout == 'gallery_block' %}<div class="galleryblock"><div class="row medium-up-2">{% for image in item.gallery %}<div class="column">{% if image %}<img src="{{TimberImage(image)}}" /><h4>{{image.caption}}</h4>{% endif %}</div>{% endfor %}</div></div>  {% elseif item.acf_fc_layout == 'button' %}<div class="row"><div class="medium-12 columns">{% if item.website_link %}<a href="{{item.website_link}}"><div class="button medium"><span>Click to view site</span></div></a>{% endif %}  {% endif %}</div></div>{% endfor %}</section>{% endblock %}